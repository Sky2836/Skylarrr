Eggs1:CreateLabel("Select the Egg")


Eggs1:CreateButton("Remove opening egg animation", img, function()
    for i,v in pairs(getgc(true)) do
        if (typeof(v) == 'table' and rawget(v, 'OpenEgg')) then
            v.OpenEgg = function()
                return
            end
        end
    end
end)

Eggs1:CreateToggle("Auto Triple Egg | if u have gamepass", img, function(triplegamepassfunc)
    _G.TripleEggs = triplegamepassfunc
    print(triplegamepassfunc)
end)



Eggs1:CreateToggle("Start Auto Egg", img, function(openeggfunc)
    _G.OpenEgg = openeggfunc
    local eggRemote=game: GetService ("Workspace")["__THINGS"]["__REMOTES"]["buy egg"]

    while wait () do
        eggRemote:InvokeServer({_G.Egg, _G.TripleEggs})
        if not _G.OpenEgg then
            break
        end
    end
    print(openeggfunc)
end)

Eggs1:CreateLabel("Try open manual first if Auto Egg not work!!!")

