local AreaList = {"VIP", "Town", "Forest", "Beach", "Mine", "Winter", "Glacier", "Desert", "Volcano", "Enchanted Forest", "Ancient Island", "Samurai Island", "Candy Island", "Haunted Island", "Hell Island", "Heaven Island", "Tech City", "Dark Tech", "Steampunk", "Alien Lab", "Alien Forest"}
local WorldList = {"Spawn", "Fantady", "Tech"}

Autofarm:CreateDropdown("Select World", img, WorldList, function(world)
    _G.World = world
    print(world)
end)
Autofarm:CreateDropdown("Select Area", img, AreaList, function(area)
    _G.Area = area
    print(area)
end)
Autofarm:CreateLabel("Select The World and Area First")

function FarmCoin(CoinID, PetID)
    game.workspace['__THINGS']['__REMOTES']["join coin"]:InvokeServer({[1] = CoinID, [2] = {[1] = PetID}})
    game.workspace['__THINGS']['__REMOTES']["farm coin"]:FireServer({[1] = CoinID, [2] = PetID})
end
function GetMyPets()
    local returntable = {}
    for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Inventory.Frame.Main.Pets:GetChildren()) do
        if v.ClassName == 'TextButton' and v.Equipped.Visible then
            table.insert(returntable, v.Name)
        end
    end
    return returntable
end
function GetCoins(world, area)
    local returntable = {}
    local ListCoins = game.workspace['__THINGS']['__REMOTES']["get coins"]:InvokeServer({})[1]
    for i,v in pairs(ListCoins) do
        if string.lower(v.w) == string.lower(world) and string.lower(v.a) == string.lower(area) then
            table.insert(returntable, i)
        end
    end
    return returntable
end
function CollectOrbs()
    local ohTable1 = {[1] = {}}
    for i,v in pairs(game.workspace['__THINGS'].Orbs:GetChildren()) do
        ohTable1[1][i] = v.Name
    end
        game.workspace['__THINGS']['__REMOTES']["claim orbs"]:FireServer(ohTable1)
end

Autofarm:CreateToggle("Start Auto Farm", img, function(autofarmfunc)
    _G.Start = autofarmfunc
    while wait() and _G.Start do
    local cointhiny = GetCoins(_G.World, _G.Area)
    local pethingy = GetMyPets()
    for i = 1, #cointhiny do
        pcall(function() FarmCoin(cointhiny[i], pethingy[i%#pethingy+1]) end)
        end
    end
    print(autofarmfunc)
end)
Autofarm:CreateLabel("If Auto Farm not work,")
Autofarm:CreateLabel("Try to break one chest first.!")


Autofarm:CreateToggle("Auto Collect Coins", img, function(orbfunc)
    _G.Orbs = orbfunc
    while wait() and _G.Orbs do
    local cointhiny = GetCoins(_G.World, _G.Area)
    local pethingy = GetMyPets()
    for i = 1, #cointhiny do
        pcall(function() CollectOrbs() end)
        end
    end
    print(orbfunc)
end)

Autofarm:CreateToggle("Auto Lootbag", img, function(autolootbagfunc)
    _G.Lootbags = autolootbagfunc
    local Running = {}
        while wait() and CoLoot do
            for i, v in pairs(game:GetService("Workspace")["__THINGS"].Lootbags:GetChildren()) do
                spawn(function()
                    if v ~= nil and v.ClassName == 'MeshPart' then
                        if not Running[v.Name] then
                            Running[v.Name] = true
                            local StartTick = tick()
                            v.Transparency = 1
                            for a,b in pairs(v:GetChildren()) do
                                if not string.find(b.Name, "Body") then
                                    b:Destroy()
                                end
                            end
                            repeat task.wait()
                                v.CFrame = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
                            until v == nil or not v.Parent or tick() > StartTick + 3
                            Running[v.Name] = nil
                        end
                    end
                end)
            end
        end
end)



