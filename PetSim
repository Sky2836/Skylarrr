do
    if game:GetService("CoreGui"):FindFirstChild("OCMenu") then
        game:GetService("CoreGui"):FindFirstChild("OCMenu"):Destroy()
    end
    if game:GetService("CoreGui"):FindFirstChild("SkyUI") then
        game:GetService("CoreGui"):FindFirstChild("SkyUI"):Destroy()
    end
end

